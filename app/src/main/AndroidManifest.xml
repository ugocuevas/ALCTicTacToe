<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ugochi.alctictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InputAlias"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".InputAliases"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".GameBoardThree" />
        <activity android:name=".ChooseBoard" />
        <activity android:name=".GameBoardFive" />
        <activity android:name=".ViewInstructions"></activity>
    </application>

</manifest>